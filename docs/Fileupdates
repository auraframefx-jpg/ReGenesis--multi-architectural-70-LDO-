        "    ) {\n",
        "        Row(\n",
        "            modifier = Modifier\n",
        "                .fillMaxWidth()\n",
        "                .padding(16.dp),\n",
        "            verticalAlignment = Alignment.CenterVertically\n",
        "        ) {\n",
        "            Text(title, style = MaterialTheme.typography.bodyLarge)\n",
        "        }\n",
        "    }\n",
        "}\n",
