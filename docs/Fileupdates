      "source": [
        "import os\n",
        "\n",
        "proguard_rules_path = \"ReGenesis--multi-architectural-70-LDO-/app/proguard-rules.pro\"\n",
        "\n",
        "proguard_rules_to_add = \"\"\"\n",
        "# YukiHookAPI ProGuard Rules\n",
        "-keep @dev.max textSize.yuuki.hook.api.annotation.InjectYukiHookWithXposed class * {*;}\n",
        "-keep class * implements dev.max textSize.yuuki.hook.api.IXposedHookLoadPackage {*;}\n",
        "-keep class * implements dev.max textSize.yuuki.hook.api.IYukiHookXposedInit {*;}\n",
        "-keep class dev.aurakai.auraframefx.genesis.oracledrive.ai.** {*;}\n",
        "\"\"\"\n",
        "\n",
        "# Ensure the directory exists\n",
        "os.makedirs(os.path.dirname(proguard_rules_path), exist_ok=True)\n",
        "\n",
        "# Read existing content or create the file if it doesn't exist\n",
        "existing_content = \"\"\n",
        "if os.path.exists(proguard_rules_path):\n",
        "    with open(proguard_rules_path, \"r\") as f:\n",
        "        existing_content = f.read()\n",
        "\n",
        "# Check if the rules already exist to prevent duplication\n",
        "if proguard_rules_to_add.strip() in existing_content.strip():\n",
        "    print(f\"ProGuard rules for YukiHookAPI already exist in {proguard_rules_path}. Skipping.\")\n",
        "else:\n",
        "    # Append the new rules\n",
        "    with open(proguard_rules_path, \"a\") as f:\n",
        "        f.write(proguard_rules_to_add)\n",
        "    print(f\"Added ProGuard rules for YukiHookAPI to {proguard_rules_path}.\")\n"
      ],
      "execution_count": null,
      "outputs": [
