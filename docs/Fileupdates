            "    }\n",
            "\n",
            "\n",
            "    private fun updateTile() {\n",
            "        val tile = qsTile\n",
            "        if (tile == null) {\n",
            "            Log.e(TAG, \"qsTile is null in updateTile\")\n",
            "            return\n",
            "        }\n",
            "\n",
            "        val currentPresetName = when (CurrentSpacingPreset.currentPreset) {\n",
