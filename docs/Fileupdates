      "source": [
        "import os\n",
        "import xml.etree.ElementTree as ET\n",
        "\n",
        "file_path = \"ReGenesis--multi-architectural-70-LDO-/app/src/main/res/values/arrays.xml\"\n",
        "\n",
        "xposed_scope_content = \"\"\"\n",
        "    <string-array name=\"xposed_scope\">\n",
        "        <item>android</item>\n",
        "        <item>com.android.systemui</item>\n",
        "        <item>dev.aurakai.auraframefx</item>\n",
        "        <!-- Add any other applications requiring hooks here -->\n",
        "    </string-array>\n",
        "\"\"\"\n",
        "\n",
        "# Ensure the directory exists (already done in the previous step, but safe to re-call)\n",
        "os.makedirs(os.path.dirname(file_path), exist_ok=True)\n",
        "\n",
        "updated = False\n",
        "\n",
        "if os.path.exists(file_path):\n",
        "    try:\n",
        "        tree = ET.parse(file_path)\n",
        "        root = tree.getroot()\n",
        "\n",
        "        # Check if resources tag is the root\n",
        "        if root.tag != 'resources':\n",
        "            print(f\"Warning: {file_path} exists but its root tag is not 'resources'. Creating new file.\")\n",
        "            # Proceed to create new file as existing one is malformed or unexpected\n",
        "            pass # This will fall through to the else block for file creation\n",
        "\n",
        "        # Try to find existing xposed_scope\n",
        "        existing_xposed_scope = root.find(\"string-array[@name='xposed_scope']\")\n",
        "\n",
        "        if existing_xposed_scope is not None:\n",
        "            # If exists, remove and re-add to update content\n",
        "            root.remove(existing_xposed_scope)\n",
        "            new_element = ET.fromstring(xposed_scope_content)\n",
        "            root.append(new_element)\n",
        "            ET.indent(tree, space=\"  \") # For pretty printing\n",
        "            tree.write(file_path, encoding='utf-8', xml_declaration=True)\n",
        "            print(f\"Updated 'xposed_scope' in {file_path}.\")\n",
        "            updated = True\n",
        "        else:\n",
        "            # If not exists, add new xposed_scope\n",
        "            new_element = ET.fromstring(xposed_scope_content)\n",
        "            root.append(new_element)\n",
        "            ET.indent(tree, space=\"  \") # For pretty printing\n",
        "            tree.write(file_path, encoding='utf-8', xml_declaration=True)\n",
        "            print(f\"Added 'xposed_scope' to existing {file_path}.\")\n",
        "            updated = True\n",
        "\n",
        "    except ET.ParseError as e:\n",
        "        print(f\"Error parsing existing XML file {file_path}: {e}. Creating new file.\")\n",
        "        # Fall through to create new file\n",
        "\n",
        "if not updated:\n",
        "    # Create a new file or overwrite if parsing failed/root was not resources\n",
        "    new_file_content = f\"\"\"<?xml version='1.0' encoding='utf-8'?>\\n<resources>\\n{xposed_scope_content}</resources>\\n\"\"\"\n",
        "    with open(file_path, \"w\") as f:\n",
        "        f.write(new_file_content)\n",
        "    print(f\"Created new {file_path} with 'xposed_scope'.\")\n"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
