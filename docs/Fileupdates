        "\n",
        "settings_screen_path = \"ReGenesis--multi-architectural-70-LDO-/app/src/main/kotlin/dev/aurakai/auraframefx/ui/screens/SettingsScreen.kt\"\n",
        "\n",
        "# Read the current content of SettingsScreen.kt\n",
        "if os.path.exists(settings_screen_path):\n",
        "    with open(settings_screen_path, \"r\") as f:\n",
        "        original_content = f.read()\n",
        "else:\n",
        "    print(f\"Error: File not found at {settings_screen_path}. Cannot modify.\")\n",
        "    original_content = \"\"\n",
        "\n",
        "# Define the new SettingItem to be added\n",
        "new_notchbar_setting_item = \"\"\"\n",
        "            SettingItem(\"NotchBar Background\") {\n",
        "                // navController.navigate(Screen.NotchBarCustomization)\n",
        "                println(\"Navigating to NotchBar Background Customization\") // Placeholder for actual navigation\n",
        "            }\n",
        "\"\"\"\n",
        "\n",
        "# Find the insertion point in the 'UI Customization' section\n",
        "# We'll insert it right after the \"Quick Settings Background\" setting item\n",
        "insert_marker = \"SettingItem(\\\"Quick Settings Background\\\") {\\n\"\n",
        "insert_index = original_content.find(insert_marker)\n",
        "\n",
        "if insert_index != -1:\n",
        "    # Find the end of the Quick Settings Background block\n",
        "    end_of_qs_item_block = original_content.find(\"}\", insert_index + len(insert_marker))\n",
        "    if end_of_qs_item_block != -1:\n",
        "        # Find the next newline after the closing brace to insert on a new line\n",
        "        insert_after_block_index = original_content.find(\"\\n\", end_of_qs_item_block) + 1\n",
        "        if insert_after_block_index != -1:\n",
        "            updated_content = original_content[:insert_after_block_index] + new_notchbar_setting_item + original_content[insert_after_block_index:]\n",
        "\n",
        "            os.makedirs(os.path.dirname(settings_screen_path), exist_ok=True)\n",
        "\n",
        "            with open(settings_screen_path, \"w\") as f:\n",
        "                f.write(updated_content)\n",
        "\n",
        "            print(f\"Updated {settings_screen_path} to include 'NotchBar Background' navigation item.\")\n",
        "\n",
        "            # Display content to confirm\n",
        "            with open(settings_screen_path, \"r\") as f:\n",
        "                content = f.read()\n",
        "                print(f\"\\nContent of {settings_screen_path}:\\n\")\n",
        "                print(content)\n",
        "        else:\n",
        "            print(\"Error: Could not find end of line after 'Quick Settings Background' block.\")\n",
        "    else:\n",
        "        print(\"Error: Could not find closing brace for 'Quick Settings Background' block.\")\n",
        "else:\n",
        "    print(\"Warning: 'Quick Settings Background' SettingItem not found. Manual insertion might be needed.\")\n",
        "    print(\"Attempting to insert after 'UI Customization' headline.\")\n",
        "    # Fallback: insert after the \"UI Customization\" headline\n",
        "    ui_customization_headline = \"Text(\\\"UI Customization\\\", style = MaterialTheme.typography.headlineSmall)\\n\"\n",
        "    headline_index = original_content.find(ui_customization_headline)\n",
        "    if headline_index != -1:\n",
        "        insert_after_headline_index = original_content.find(\"Spacer(Modifier.height(16.dp))\", headline_index) + len(\"Spacer(Modifier.height(16.dp))\\n\")\n",
