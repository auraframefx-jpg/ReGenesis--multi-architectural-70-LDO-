      "source": [
        "import os\n",
        "\n",
        "file_path = \"ReGenesis--multi-architectural-70-LDO-/app/src/main/kotlin/dev/aurakai/auraframefx/ui/screens/GateCustomizationScreen.kt\"\n",
        "\n",
        "# Read the current content of the GateCustomizationScreen.kt file\n",
        "if os.path.exists(file_path):\n",
        "    with open(file_path, \"r\") as f:\n",
        "        original_content = f.read()\n",
        "else:\n",
        "    print(f\"Error: File not found at {file_path}. Cannot modify.\")\n",
        "    original_content = \"\"\n",
        "\n",
        "modified_content = original_content\n",
        "\n",
        "# 1. Remove the 'gateSpacingConfig' initialization block\n",
        "start_idx = modified_content.find(\"    val gateSpacingConfig = SpacingConfig(\")\n",
        "if start_idx != -1:\n",
        "    balance = 0\n",
        "    end_idx = -1\n",
        "    for i in range(start_idx, len(modified_content)):\n",
        "        if modified_content[i] == '(':\n",
        "            balance += 1\n",
        "        elif modified_content[i] == ')':\n",
        "            balance -= 1\n",
        "        if balance == 0 and modified_content[i] == ')':\n",
        "            end_idx = i\n",
        "            break\n",
        "    if end_idx != -1:\n",
        "        # Find the end of the line after the closing parenthesis\n",
        "        end_line_idx = modified_content.find(\"\\n\", end_idx)\n",
        "        if end_line_idx == -1:\n",
        "            end_line_idx = len(modified_content)\n",
        "\n",
        "        # Remove the whole declaration block including the newline after it\n",
        "        modified_content = modified_content[:start_idx] + modified_content[end_line_idx + 1:]\n",
        "        print(\"Removed 'gateSpacingConfig' declaration block.\")\n",
        "    else:\n",
        "        print(\"Warning: Could not find closing parenthesis for 'gateSpacingConfig' declaration.\")\n",
        "else:\n",
        "    print(\"Warning: 'gateSpacingConfig' declaration not found.\")\n",
        "\n",
        "\n",
        "# 2. Modify the SpacingControlPanel call to remove 'currentSpacingConfig' parameter\n",
        "# and add the 'modifier' parameter\n",
        "\n",
        "# Find the start of the SpacingControlPanel call block\n",
        "spacing_control_panel_call_start = modified_content.find(\"            SpacingControlPanel(\")\n",
        "if spacing_control_panel_call_start != -1:\n",
        "    # Find the line that assigns currentSpacingConfig\n",
        "    current_spacing_config_line_start = modified_content.find(\"                currentSpacingConfig = gateSpacingConfig,\", spacing_control_panel_call_start)\n",
        "    if current_spacing_config_line_start != -1:\n",
        "        current_spacing_config_line_end = modified_content.find(\"\\n\", current_spacing_config_line_start)\n",
        "        if current_spacing_config_line_end != -1:\n",
        "            # Replace that line with the new modifier line\n",
        "            replacement_line = \"\"\"                modifier = Modifier.fillMaxWidth().padding(horizontal = 16.dp),\"\"\"\n",
        "            modified_content = (modified_content[:current_spacing_config_line_start] +\n",
        "                                replacement_line +\n",
        "                                modified_content[current_spacing_config_line_end:])\n",
        "            print(\"Modified SpacingControlPanel call to remove currentSpacingConfig and add modifier.\")\n",
        "        else:\n",
        "            print(\"Warning: Could not find end of 'currentSpacingConfig' line.\")\n",
        "    else:\n",
        "        print(\"Warning: 'currentSpacingConfig' parameter not found in SpacingControlPanel call.\")\n",
        "else:\n",
        "    print(\"Warning: SpacingControlPanel call not found.\")\n",
        "\n",
        "\n",
        "os.makedirs(os.path.dirname(file_path), exist_ok=True)\n",
        "\n",
        "with open(file_path, \"w\") as f:\n",
        "    f.write(modified_content)\n",
        "\n",
        "print(f\"Updated {file_path} to remove currentSpacingConfig parameter from SpacingControlPanel call.\")\n",
        "\n",
        "# Display content to confirm\n",
        "with open(file_path, \"r\") as f:\n",
        "    content = f.read()\n",
        "    print(f\"\\nContent of {file_path}:\\n\")\n",
        "    print(content)"
