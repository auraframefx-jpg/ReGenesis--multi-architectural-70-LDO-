        "import os\n",
        "\n",
        "# The list of gradle_file_paths is already defined from the previous step\n",
        "# gradle_file_paths = [\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysa/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysb/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysc/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysd/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsyse/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysf/build.gradle.kts\",\n",
        "# ]\n",
        "\n",
        "for file_path in gradle_file_paths:\n",
        "    if not os.path.exists(file_path):\n",
        "        print(f\"Warning: File not found at {file_path}. Skipping.\")\n",
        "        continue\n",
        "\n",
        "    with open(file_path, \"r\") as f:\n",
        "        content = f.read()\n",
        "\n",
        "    modified_content = content\n",
        "\n",
        "    # 1. Add id(\"genesis.android.library.hilt\") to the plugins { ... } block\n",
        "    if 'id(\"genesis.android.library.hilt\")' not in modified_content:\n",
        "        if 'plugins {' in modified_content:\n",
        "            modified_content = modified_content.replace(\n",
        "                'plugins {',\n",
        "                'plugins {\\n    id(\"genesis.android.library.hilt\")',\n",
        "                1\n",
        "            )\n",
        "        else:\n",
        "            print(f\"Warning: 'plugins {{' block not found in {file_path}. Cannot add genesis.android.library.hilt.\")\n",
        "\n",
        "    # 2. Add id(\"com.google.devrel.ksp\") to plugins { ... } if not present\n",
        "    #    This is typically required for KSP processor dependencies\n",
        "    if 'id(\"com.google.devrel.ksp\")' not in modified_content:\n",
        "        if 'plugins {' in modified_content:\n",
        "            # Try to find an existing plugin entry to place it after\n",
        "            insert_point_plugins = modified_content.find('plugins {') + len('plugins {')\n",
        "            # Find the next newline after plugins { to insert the new plugin ID\n",
        "            insert_point_plugins = modified_content.find('\\n', insert_point_plugins)\n",
        "            if insert_point_plugins != -1:\n",
        "                modified_content = modified_content[:insert_point_plugins] + '\\n    id(\"com.google.devrel.ksp\")' + modified_content[insert_point_plugins:]\n",
        "            else:\n",
        "                print(f\"Warning: Could not find suitable insertion point in 'plugins {{' block in {file_path} for KSP plugin.\")\n",
        "        else:\n",
        "            print(f\"Warning: 'plugins {{' block not found in {file_path}. Cannot add KSP plugin.\")\n",
        "\n",
        "\n",
        "    # 3. Add dependencies\n",
        "    dependencies_to_add = [\n",
        "        'implementation(libs.yukihook.api)',\n",
        "        'ksp(libs.yukihook.ksp)'\n",
        "    ]\n",
        "\n",
        "    if 'dependencies {' in modified_content:\n",
        "        dependencies_block_start = modified_content.find('dependencies {')\n",
        "        dependencies_block_end = modified_content.find('}', dependencies_block_start)\n",
        "\n",
        "        if dependencies_block_start != -1 and dependencies_block_end != -1:\n",
        "            original_dependencies_block = modified_content[dependencies_block_start : dependencies_block_end + 1]\n",
        "            new_dependencies_block = original_dependencies_block\n",
        "\n",
        "            for dep in dependencies_to_add:\n",
        "                if dep not in new_dependencies_block:\n",
        "                    # Insert dependency right before the closing brace of the dependencies block\n",
        "                    new_dependencies_block = new_dependencies_block.replace(\n",
        "                        '\\n}',\n",
        "                        f'\\n    {dep}\\n}',\n",
        "                        1\n",
        "                    )\n",
        "\n",
        "            modified_content = modified_content.replace(original_dependencies_block, new_dependencies_block)\n",
        "        else:\n",
        "            print(f\"Warning: 'dependencies {{' block malformed or not found in {file_path}. Cannot add YukiHook dependencies.\")\n",
        "    else:\n",
        "        print(f\"Warning: 'dependencies {{' block not found in {file_path}. Cannot add YukiHook dependencies.\")\n",
        "\n",
        "    with open(file_path, \"w\") as f:\n",
        "        f.write(modified_content)\n",
        "    print(f\"Updated {file_path} with genesis.android.library.hilt, YukiHook API and KSP dependencies.\")"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "error",
          "ename": "SyntaxError",
          "evalue": "f-string: single '}' is not allowed (ipython-input-1586711858.py, line 69)",
          "traceback": [
            "\u001b[0;36m  File \u001b[0;32m\"/tmp/ipython-input-1586711858.py\"\u001b[0;36m, line \u001b[0;32m69\u001b[0m\n\u001b[0;31m    f'\\n    {dep}\\n}',\u001b[0m\n\u001b[0m                   ^\u001b[0m\n\u001b[0;31mSyntaxError\u001b[0m\u001b[0;31m:\u001b[0m f-string: single '}' is not allowed\n"
          ]
        }
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "31a910d0"
      },
      "source": [
        "**Reasoning**:\n",
        "The previous code failed due to a `SyntaxError` because a literal closing curly brace `}` was not properly escaped within an f-string. To fix this, I will escape the literal curly brace by doubling it `}}` within the f-string.\n",
        "\n"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "b96dc50f",
        "outputId": "e4c57aa2-b920-46ec-b494-5611d63753e2"
      },
      "source": [
        "import os\n",
        "\n",
        "# The list of gradle_file_paths is already defined from the previous step\n",
        "# gradle_file_paths = [\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysa/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysb/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysc/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysd/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsyse/build.gradle.kts\",\n",
        "#     \"ReGenesis--multi-architectural-70-LDO-/extendsysf/build.gradle.kts\",\n",
        "# ]\n",
        "\n",
        "for file_path in gradle_file_paths:\n",
        "    if not os.path.exists(file_path):\n",
        "        print(f\"Warning: File not found at {file_path}. Skipping.\")\n",
        "        continue\n",
        "\n",
        "    with open(file_path, \"r\") as f:\n",
        "        content = f.read()\n",
        "\n",
        "    modified_content = content\n",
        "\n",
        "    # 1. Add id(\"genesis.android.library.hilt\") to the plugins { ... } block\n",
        "    if 'id(\"genesis.android.library.hilt\")' not in modified_content:\n",
        "        if 'plugins {' in modified_content:\n",
        "            modified_content = modified_content.replace(\n",
        "                'plugins {',\n",
        "                'plugins {\\n    id(\"genesis.android.library.hilt\")',\n",
        "                1\n",
        "            )\n",
        "        else:\n",
        "            print(f\"Warning: 'plugins {{' block not found in {file_path}. Cannot add genesis.android.library.hilt.\")\n",
        "\n",
        "    # 2. Add id(\"com.google.devrel.ksp\") to plugins { ... } if not present\n",
        "    #    This is typically required for KSP processor dependencies\n",
        "    if 'id(\"com.google.devrel.ksp\")' not in modified_content:\n",
        "        if 'plugins {' in modified_content:\n",
        "            # Try to find an existing plugin entry to place it after\n",
        "            insert_point_plugins = modified_content.find('plugins {') + len('plugins {')\n",
        "            # Find the next newline after plugins { to insert the new plugin ID\n",
        "            insert_point_plugins = modified_content.find('\\n', insert_point_plugins)\n",
        "            if insert_point_plugins != -1:\n",
        "                modified_content = modified_content[:insert_point_plugins] + '\\n    id(\"com.google.devrel.ksp\")' + modified_content[insert_point_plugins:]\n",
        "            else:\n",
        "                print(f\"Warning: Could not find suitable insertion point in 'plugins {{' block in {file_path} for KSP plugin.\")\n",
        "        else:\n",
        "            print(f\"Warning: 'plugins {{' block not found in {file_path}. Cannot add KSP plugin.\")\n",
        "\n",
        "\n",
        "    # 3. Add dependencies\n",
        "    dependencies_to_add = [\n",
        "        'implementation(libs.yukihook.api)',\n",
        "        'ksp(libs.yukihook.ksp)'\n",
        "    ]\n",
        "\n",
        "    if 'dependencies {' in modified_content:\n",
        "        dependencies_block_start = modified_content.find('dependencies {')\n",
        "        dependencies_block_end = modified_content.find('}', dependencies_block_start)\n",
        "\n",
        "        if dependencies_block_start != -1 and dependencies_block_end != -1:\n",
        "            original_dependencies_block = modified_content[dependencies_block_start : dependencies_block_end + 1]\n",
        "            new_dependencies_block = original_dependencies_block\n",
        "\n",
        "            for dep in dependencies_to_add:\n",
        "                if dep not in new_dependencies_block:\n",
        "                    # Insert dependency right before the closing brace of the dependencies block\n",
        "                    new_dependencies_block = new_dependencies_block.replace(\n",
        "                        '\\n}',\n",
        "                        f'\\n    {dep}\\n}}',\n",
        "                        1\n",
        "                    )\n",
        "\n",
        "            modified_content = modified_content.replace(original_dependencies_block, new_dependencies_block)\n",
        "        else:\n",
        "            print(f\"Warning: 'dependencies {{' block malformed or not found in {file_path}. Cannot add YukiHook dependencies.\")\n",
        "    else:\n",
        "        print(f\"Warning: 'dependencies {{' block not found in {file_path}. Cannot add YukiHook dependencies.\")\n",
        "\n",
        "    with open(file_path, \"w\") as f:\n",
        "        f.write(modified_content)\n",
        "    print(f\"Updated {file_path} with genesis.android.library.hilt, YukiHook API and KSP dependencies.\")"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Updated ReGenesis--multi-architectural-70-LDO-/extendsysa/build.gradle.kts with genesis.android.library.hilt, YukiHook API and KSP dependencies.\n",
            "Updated ReGenesis--multi-architectural-70-LDO-/extendsysb/build.gradle.kts with genesis.android.library.hilt, YukiHook API and KSP dependencies.\n",
            "Updated ReGenesis--multi-architectural-70-LDO-/extendsysc/build.gradle.kts with genesis.android.library.hilt, YukiHook API and KSP dependencies.\n",
            "Updated ReGenesis--multi-architectural-70-LDO-/extendsysd/build.gradle.kts with genesis.android.library.hilt, YukiHook API and KSP dependencies.\n",
            "Updated ReGenesis--multi-architectural-70-LDO-/extendsyse/build.gradle.kts with genesis.android.library.hilt, YukiHook API and KSP dependencies.\n",
            "Updated ReGenesis--multi-architectural-70-LDO-/extendsysf/build.gradle.kts with genesis.android.library.hilt, YukiHook API and KSP dependencies.\n"
          ]
