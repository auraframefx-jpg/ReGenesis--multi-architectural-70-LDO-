      "source": [
        "import os\n",
        "\n",
        "preferences_file_path = \"ReGenesis--multi-architectural-70-LDO-/app/src/main/kotlin/dev/aurakai/auraframefx/preferences/CustomizationPreferences.kt\"\n",
        "\n",
        "# Read the current content of the CustomizationPreferences.kt file\n",
        "if os.path.exists(preferences_file_path):\n",
        "    with open(preferences_file_path, \"r\") as f:\n",
        "        original_content = f.read()\n",
        "else:\n",
        "    print(f\"Error: File not found at {preferences_file_path}. Cannot modify.\")\n",
        "    # Fallback to creating a new file with basic content if not found\n",
        "    original_content = \"\"\"\n",
        "package dev.aurakai.auraframefx.preferences\n",
        "\n",
        "import android.content.Context\n",
        "import android.net.Uri\n",
        "import androidx.compose.ui.layout.ContentScale\n",
        "import androidx.core.content.edit\n",
        "\n",
        "object CustomizationPreferences {\n",
        "\n",
        "    private const val PREFS_NAME = \"customization_prefs\"\n",
        "    private const val KEY_HEADER_IMAGE_URI = \"header_image_uri\"\n",
        "    private const val KEY_HEADER_IMAGE_SCALE = \"header_image_scale\"\n",
        "\n",
        "    fun saveHeaderImage(context: Context, uri: Uri?, scale: ContentScale) {\n",
        "        context.getSharedPreferences(PREFS_NAME, Context.MODE_PRIVATE).edit {\n",
        "            putString(KEY_HEADER_IMAGE_URI, uri?.toString())\n",
        "            putString(KEY_HEADER_IMAGE_SCALE, scale.name)\n",
        "            apply()\n",
        "        }\n",
        "    }\n",
        "\n",
        "    fun getHeaderImageUri(context: Context): Uri? {\n",
        "        val uriString = context.getSharedPreferences(PREFS_NAME, Context.MODE_PRIVATE)\n",
        "            .getString(KEY_HEADER_IMAGE_URI, null)\n",
        "        return uriString?.let { Uri.parse(it) }\n",
        "    }\n",
        "\n",
        "    fun getHeaderImageScale(context: Context): ContentScale {\n",
        "        val scaleName = context.getSharedPreferences(PREFS_NAME, Context.MODE_PRIVATE)\n",
        "            .getString(KEY_HEADER_IMAGE_SCALE, ContentScale.Crop.name)\n",
        "        return ContentScale.values().firstOrNull { it.name == scaleName } ?: ContentScale.Crop\n",
        "    }\n",
        "}\n",
        "\"\"\"\n",
        "\n",
        "# Ensure Gson import for serialization/deserialization\n",
        "if \"import com.google.gson.Gson\" not in original_content:\n",
        "    insert_import_point = original_content.find(\"import android.net.Uri\")\n",
        "    if insert_import_point != -1:\n",
        "        original_content = original_content[:insert_import_point] + \\\n",
        "            \"import com.google.gson.Gson\\n\" + \\\n",
        "            original_content[insert_import_point:]\n",
        "\n",
        "# Ensure SpacingConfig import\n",
        "if \"import dev.aurakai.auraframefx.aura.lab.SpacingConfig\" not in original_content:\n",
        "    insert_import_point = original_content.find(\"import android.net.Uri\")\n",
        "    if insert_import_point != -1:\n",
        "        original_content = original_content[:insert_import_point] + \\\n",
        "            \"import dev.aurakai.auraframefx.aura.lab.SpacingConfig\\n\" + \\\n",
        "            original_content[insert_import_point:]\n",
        "\n",
        "# New keys and methods for SpacingConfig\n",
        "new_spacing_config_methods = \"\"\"\n",
        "    private const val KEY_SPACING_CONFIG = \"spacing_config\"\n",
        "    private val gson = Gson()\n",
        "\n",
        "    fun saveSpacingConfig(context: Context, spacingConfig: SpacingConfig) {\n",
        "        context.getSharedPreferences(PREFS_NAME, Context.MODE_PRIVATE).edit {\n",
        "            putString(KEY_SPACING_CONFIG, gson.toJson(spacingConfig))\n",
        "            apply()\n",
        "        }\n",
        "    }\n",
        "\n",
        "    fun getSpacingConfig(context: Context): SpacingConfig {\n",
        "        val jsonString = context.getSharedPreferences(PREFS_NAME, Context.MODE_PRIVATE)\n",
        "            .getString(KEY_SPACING_CONFIG, null)\n",
        "        return if (jsonString != null) {\n",
        "            gson.fromJson(jsonString, SpacingConfig::class.java)\n",
        "        } else {\n",
        "            SpacingConfig() // Return default if not found\n",
        "        }\n",
        "    }\n",
        "\"\"\"\n",
        "\n",
        "# Find the last closing brace of the object to insert the new content before it\n",
        "object_end_index = original_content.rfind(\"}\")\n",
        "\n",
        "if object_end_index != -1:\n",
        "    # Insert the new keys and methods before the last closing brace\n",
        "    updated_content = original_content[:object_end_index] + new_spacing_config_methods + \"\\n}\"\n",
        "else:\n",
        "    updated_content = original_content # No object definition found, no change\n",
        "    print(\"Error: Could not find object CustomizationPreferences definition.\")\n",
        "\n",
        "# Ensure the directory exists (already done, but safe for standalone execution)\n",
        "os.makedirs(os.path.dirname(preferences_file_path), exist_ok=True)\n",
        "\n",
        "with open(preferences_file_path, \"w\") as f:\n",
        "    f.write(updated_content)\n",
        "\n",
        "print(f\"Updated {preferences_file_path} with SpacingConfig persistence methods.\")"
      ],
      "execution_count": null,
      "outputs": [
