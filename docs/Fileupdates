    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "1537db63",
        "outputId": "116140ca-64cf-44e9-a832-f17c0b947da2"
      },
      "source": [
        "import os\n",
        "\n",
        "file_path = \"ReGenesis--multi-architectural-70-LDO-/domains/aura/ui/components/TransformedImage.kt\"\n",
        "\n",
        "transformed_image_content = \"\"\"\n",
        "package dev.aurakai.auraframefx.aura.ui.components\n",
        "\n",
        "import android.net.Uri\n",
        "import androidx.compose.foundation.Image\n",
        "import androidx.compose.foundation.layout.BoxWithConstraints\n",
        "import androidx.compose.foundation.layout.fillMaxSize\n",
        "import androidx.compose.runtime.Composable\n",
        "import androidx.compose.ui.Modifier\n",
        "import androidx.compose.ui.graphics.graphicsLayer\n",
        "import androidx.compose.ui.layout.ContentScale\n",
        "import coil.compose.rememberAsyncImagePainter\n",
        "import coil.request.ImageRequest\n",
        "import coil.transform.CropTransformation\n",
        "import androidx.compose.ui.platform.LocalContext\n",
        "\n",
        "import dev.aurakai.auraframefx.aura.lab.ImageTransformation\n",
        "\n",
        "@Composable\n",
        "fun TransformedImage(\n",
        "    uri: Uri?,\n",
        "    transformation: ImageTransformation,\n",
        "    modifier: Modifier = Modifier,\n",
        "    contentDescription: String? = null,\n",
        "    contentScale: ContentScale = ContentScale.Crop\n",
        ") {\n",
        "    val context = LocalContext.current\n",
        "\n",
        "    BoxWithConstraints(modifier = modifier) {\n",
        "        // Calculate original image dimensions or use default if not available for accurate crop\n",
        "        // In a real scenario, you might get actual image dimensions from metadata or painterState\n",
        "        val originalWidth = constraints.maxWidth.toFloat()\n",
        "        val originalHeight = constraints.maxHeight.toFloat()\n",
        "\n",
        "        // Create ImageRequest with CropTransformation\n",
        "        val imageRequest = ImageRequest.Builder(context)\n",
        "            .data(uri)\n",
        "            .transformations(listOf(createCropTransformation(transformation, originalWidth, originalHeight)))\n",
        "            .build()\n",
        "\n",
        "        val painter = rememberAsyncImagePainter(imageRequest)\n",
        "\n",
        "        Image(\n",
        "            painter = painter,\n",
        "            contentDescription = contentDescription,\n",
        "            modifier = Modifier\n",
        "                .fillMaxSize()\n",
        "                .graphicsLayer(\n",
        "                    rotationZ = transformation.rotation,\n",
        "                    scaleX = transformation.scale * if (transformation.flipHorizontal) -1f else 1f,\n",
        "                    scaleY = transformation.scale * if (transformation.flipVertical) -1f else 1f\n",
        "                ),\n",
        "            contentScale = contentScale\n",
        "        )\n",
        "    }\n",
        "}\n",
        "\n",
        "private fun createCropTransformation(\n",
        "    transformation: ImageTransformation,\n",
        "    originalWidth: Float,\n",
        "    originalHeight: Float\n",
        "): CropTransformation {\n",
        "    // Calculate crop rectangle in pixels from percentages\n",
        "    val cropLeftPx = transformation.cropLeft * originalWidth\n",
        "    val cropTopPx = transformation.cropTop * originalHeight\n",
        "    val cropRightPx = transformation.cropRight * originalWidth\n",
        "    val cropBottomPx = transformation.cropBottom * originalHeight\n",
        "\n",
        "    val cropWidth = cropRightPx - cropLeftPx\n",
        "    val cropHeight = cropBottomPx - cropTopPx\n",
        "\n",
        "    return CropTransformation(\n",
        "        x = cropLeftPx,\n",
        "        y = cropTopPx,\n",
        "        width = cropWidth,\n",
        "        height = cropHeight\n",
        "    )\n",
        "}\n",
        "\"\"\"\n",
        "\n",
        "# Ensure the directory exists (already done, but safe for standalone execution)\n",
        "os.makedirs(os.path.dirname(file_path), exist_ok=True)\n",
        "\n",
        "with open(file_path, \"w\") as f:\n",
        "    f.write(transformed_image_content)\n",
        "\n",
        "print(f\"Created/Updated {file_path} with TransformedImage Composable.\")"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Created/Updated ReGenesis--multi-architectural-70-LDO-/domains/aura/ui/components/TransformedImage.kt with TransformedImage Composable.\n"
          ]
        }
      ]
    },
